{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": true,
    "target": "es5",
    "lib": [ "dom", "es5", "scripthost", "es2015.core", "es2015.promise", "es2015.reflect" ],
    "module": "none",
    "moduleResolution": "classic",
    "outDir": "wwwroot/js"
  },
  "typeAcquisition": {
    "enable": true
  },
  "exclude": [
    "./node_modules/**"
  ]
}
